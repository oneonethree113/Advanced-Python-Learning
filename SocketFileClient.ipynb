    "import socket\n",
    "host='localhost'\n",
    "port=8080\n",
    "\n",
    "\n",
    "s=socket.socket(socket.AF_INET,socket.SOCK_STREAM)\n",
    "s.connect((host,port))\n",
    "\n",
    "fileNmae='test.txt'\n",
    "s.send(fileNmae.encode())\n",
    "readfile=s.recv(1024)\n",
    "print(readfile.decode())\n",
    "s.close()\n",
    "\n"
   ]
